---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<urlset xmlns="http://www.sitemaps.org/schemas/sitemap/0.9">
  {% for page in site.pages %}
    {% unless page.url contains '.xml' or page.url contains '.css' or page.url contains '.js' or page.url contains 'assets' %}
    {% if page.url and page.layout %}
    <url>
      <loc>{{ page.url | absolute_url }}</loc>
      {% if page.date %}
      <lastmod>{{ page.date | date_to_xmlschema }}</lastmod>
      {% else %}
      <lastmod>{{ site.time | date_to_xmlschema }}</lastmod>
      {% endif %}
      <changefreq>monthly</changefreq>
      <priority>{% if page.url == '/' %}1.0{% elsif page.url contains 'contact' or page.url contains 'bathroom' or page.url contains 'kitchen' %}0.9{% elsif page.url contains 'areas' %}0.8{% elsif page.url contains 'guides' %}0.7{% else %}0.6{% endif %}</priority>
    </url>
    {% endif %}
    {% endunless %}
  {% endfor %}
</urlset>
